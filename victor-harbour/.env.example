# AWS Configuration for S3 Deployment
# Copy this file to .env and fill in your actual values

# AWS Credentials (from IAM user created for deployment)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# AWS Region where your S3 bucket is located
AWS_REGION=us-east-1

# S3 Bucket name for hosting your static website
AWS_S3_BUCKET=your-bucket-name-here

# CloudFront Distribution ID (optional, for CDN cache invalidation)
# Leave empty if not using CloudFront
AWS_CLOUDFRONT_DISTRIBUTION_ID=

# Example values:
# AWS_ACCESS_KEY_ID=AKIAIOSFODNN7EXAMPLE
# AWS_SECRET_ACCESS_KEY=wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY
# AWS_REGION=us-east-1
# AWS_S3_BUCKET=my-tour-site-bucket
# AWS_CLOUDFRONT_DISTRIBUTION_ID=E1234567890123